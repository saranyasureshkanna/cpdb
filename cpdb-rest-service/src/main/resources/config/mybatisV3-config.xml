<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN" "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
	<settings>
        <setting name="jdbcTypeForNull" value="NULL" />
    </settings>
	<typeAliases>
		<typeAlias type="com.kohler.persistence.domain.ItemGroups" alias="ItemGroups"/>
		<typeAlias type="com.kohler.persistence.domain.ItemInfo" alias="ItemInfo"/>
		<typeAlias type="com.kohler.persistence.domain.ItemLinks" alias="ItemLinks"/>
		<typeAlias type="com.kohler.persistence.domain.ItemLinkTypes" alias="ItemLinkTypes"/>
	</typeAliases>
    <mappers>
          <mapper resource="maps/ItemGroupsMapperPuni.xml" />
          <mapper resource="maps/ItemInfoMapper.xml" />     
          <mapper resource="maps/ItemGroupsMapperPcen.xml" />
          <mapper resource="maps/ItemLinksMapperPcen.xml" />
          <mapper resource="maps/ItemLinksMapperPuni.xml" />
    </mappers>
</configuration>